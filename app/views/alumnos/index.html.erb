<p id="notice"><%= notice %></p>

<h1>Alumnos</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Dni</th>
      <th>Email</th>
      <th>Email confirmation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alumnos.each do |alumno| %>
      <tr>
        <td><%= alumno.nombres %></td>
        <td><%= alumno.dni %></td>
        <td><%= alumno.email %></td>
        <td><%= alumno.email_confirmation %></td>
        <td><%= link_to 'Show', alumno %></td>
        <td><%= link_to 'Edit', edit_alumno_path(alumno) %></td>
        <td><%= link_to 'Destroy', alumno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alumno', new_alumno_path %>
